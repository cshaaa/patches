+    bot->SetLevel(level);
+    ClearInventory();
+    bot->SetUInt32Value(PLAYER_XP, 0);
+    CancelAuras();
+    bot->SaveToDB();
+
+    InitAvailableSpells();
+    InitSkills();
+    InitTradeSkills();
+    InitTalents();
+    InitAvailableSpells();
