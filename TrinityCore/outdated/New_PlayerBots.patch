+    InitBags();
+    InitAmmo();
+    InitFood();
+    InitPotions();
+    InitSecondEquipmentSet();
+    InitInventory();
+    InitGlyphs();
+    bot->SetMoney(urand(level * 1000, level * 5 * 1000));
+    bot->SaveToDB();
+
+    InitPet();
